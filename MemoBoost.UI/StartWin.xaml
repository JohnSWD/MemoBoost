<Window x:Class="MemoBoost.UI.StartWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MemoBoost.UI"
        mc:Ignorable="d"
        Title="Memory Boost" MinHeight="300" Height="300" Width="450" MinWidth="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="15"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Margin="10 0 0 0">
            <Hyperlink Name="Home" Tag="Home" Click="Hyperlink_Click" Foreground="Black">Home</Hyperlink>
        </TextBlock>
        
        <TextBlock HorizontalAlignment="Left" Margin="50 0  10 0">
            <Hyperlink Name="cprofile" Click="Cprofile_Click" Foreground="Black">Change profile</Hyperlink>
        </TextBlock>

        <TextBlock HorizontalAlignment="Left" Margin="135 0  10 0">
            <Hyperlink Name="opstat" Click="Opstat_Click" Foreground="Black">Study statisticks</Hyperlink>
        </TextBlock>

        <Frame Grid.Row="1" Name="Main" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
